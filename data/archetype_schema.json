      "type_id": "signal.trend_pullback",
      "schema_version": 1,
      "etag": "W/\"seed-v1.signal.trend_pullback\"",
      "json_schema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object",
        "required": ["tf", "symbol", "direction", "dip_trigger", "dip_threshold"],
        "properties": {
          "tf": { "enum": ["15m", "1h", "4h", "1d"] },
          "symbol": { "type": "string" },
          "direction": { "enum": ["long", "short"] },
          "dip_trigger": { "enum": ["keltner", "bollinger", "ema_z"] },
          "dip_threshold": { "type": "number", "minimum": 0.2, "maximum": 5.0 },
          "trend_gate": {
            "oneOf": [
              {
                "type": "object",
                "properties": {
                  "mode": { "const": "hard" },
                  "gate": {
                    "type": "object",
                    "properties": {
                      "kind": { "const": "preset" },
                      "name": { "enum": ["uptrend_basic", "downtrend_basic"] }
                    },
                    "required": ["kind", "name"],
                    "additionalProperties": false
                  }
                },
                "required": ["mode", "gate"],
                "additionalProperties": false
              },
              {
                "type": "object",
                "properties": {
                  "mode": { "const": "ma_rel" },
                  "fast": { "type": "integer", "minimum": 5, "maximum": 50 },
                  "slow": { "type": "integer", "minimum": 20, "maximum": 200 },
                  "op": { "enum": [">", "<"] }
                },
                "required": ["mode", "fast", "slow", "op"],
                "additionalProperties": false
              }
            ]
          },
          "cooldown_bars": { "type": "integer", "minimum": 0, "maximum": 1000 },
          "max_entries_per_day": { "type": "integer", "minimum": 0, "maximum": 50 }
        },
        "additionalProperties": false
      },
      "constraints": { "min_history_bars": 120, "pit_safe": true },
      "slot_hints": {
        "dip_threshold": { "unit": "x_band_or_sigma", "min": 0.5, "max": 3.0, "step": 0.1 },
        "trend_gate": { "ui": "discriminated_union" }
      },
      "examples": [
        {
          "human": "On BTC 1h, buy dips ≤1.5×Keltner only when uptrend",
          "slots": { "tf": "1h", "symbol": "BTC-USD", "direction": "long", "dip_trigger": "keltner", "dip_threshold": 1.5, "trend_gate": { "mode": "hard", "gate": { "kind": "preset", "name": "uptrend_basic" } } }
        }
      ],
      "notes": [],
      "updated_at": "2025-11-25T00:00:00Z"
